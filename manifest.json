{
    "manifest_version": 3,
    "name": "URL Safety Checker",
    "version": "1.0",
    "permissions": [
      "declarativeNetRequest",
      "activeTab"
    ],
    "background": {
      "service_worker": "test.js"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["config/content.js"]
        }
    ],  
    "host_permissions": [
      "<all_urls>"
    ]
  }
  